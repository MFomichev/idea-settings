<application>
  <component name="PostfixTemplates">
    <template id="mk@userDefined" key=".when" provider="builtin.java" language-level="1.3" topmost="true">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="when($EXPR$)$END$;" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="assert@userDefined" key=".asse" provider="builtin.java" language-level="1.3" topmost="true">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="assertEquals($END$, $EXPR$);" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="assn@userDefined" key=".assn" provider="builtin.java" language-level="1.3" topmost="true">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="assertNull($EXPR$);$END$" toReformat="true" toShortenFQNames="true" />
    </template>
    <template id="assnn@userDefined" key=".assnn" provider="builtin.java" language-level="1.3" topmost="true">
      <conditions>
        <condition id="non void" />
      </conditions>
      <template name="fakeKey" value="assertNotNull($EXPR$);$END$" toReformat="true" toShortenFQNames="true" />
    </template>
  </component>
  <component name="PostfixTemplatesSettings">
    <option name="providerToDisabledTemplates">
      <disabled-templates provider="builtin.java">
        <value>
          <set>
            <option value="assert" />
          </set>
        </value>
      </disabled-templates>
    </option>
  </component>
</application>